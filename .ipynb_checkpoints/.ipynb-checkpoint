    "### GMAPS - Visualizing changes in Median Sales Price for states from 2008 - 2019"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 813,
   "metadata": {},
   "outputs": [],
   "source": [
    "total_data = total_data.reset_index()\n",
    "total_data[\"%Increase/Decrease_ZHVI\"] = ((total_data[\"2018_Avg_ZHVI\"] - total_data[\"2008_Avg_ZHVI\"])/total_data[\"2008_Avg_ZHVI\"]) * 100\n",
    "total_data[\"%Increase/Decrease_MSR\"] = ((total_data[\"2018_Avg_MSR\"] - total_data[\"2008_Avg_MSR\"])/total_data[\"2008_Avg_MSR\"]) * 100\n",
    "total_data[\"%Increase/Decrease_NS\"] = ((total_data[\"2018_Avg_NS\"] - total_data[\"2008_Avg_NS\"])/total_data[\"2008_Avg_NS\"]) * 100\n",
    "total_data[\"%Increase/Decrease_FR\"] = ((total_data[\"2018_Avg_FR\"] - total_data[\"2008_Avg_FR\"])/total_data[\"2008_Avg_FR\"]) * 100\n",
    "total_data[\"Lat\"]=\" \"\n",
    "total_data[\"Lng\"]=\" \""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 814,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Run state names through API and fetch longtitude and latitude values\n",
    "base_url = \"https://maps.googleapis.com/maps/api/geocode/json\"\n",
    "\n",
    "params = {\n",
    "    \"key\": gkey\n",
    "}\n",
    "\n",
    "for index, row in total_data.iterrows():\n",
    "        state = row[\"State\"]\n",
    "        params[\"address\"] = state\n",
    "        response = requests.get(base_url, params)\n",
    "        json_response = response.json()\n",
    "        # Add lat and lng values to new columns\n",
    "        total_data.loc[index, \"Lat\"] = json_response['results'][0]['geometry']['location']['lat']\n",
    "        total_data.loc[index, \"Lng\"] = json_response['results'][0]['geometry']['location']['lng']"
   ]
  },
