   "source": [
    "#Join all suffixes across different years to new_main_data\n",
    "years = [i for i in range(2008,2020,1)]\n",
    "suffixes = [\"ZHVI\",\"MSR\",\"NS\",\"FR\"]\n",
    "\n",
    "for year in years:\n",
    "    for suffix in suffixes:\n",
    "        new_main_data = new_main_data.join(findavg(main_data,year,suffix))\n",
    "new_main_data.head()"
   ]
  },
